{
  "name": "nethunter-fx",
  "version": "1.0.0",
  "description": "Nethunter Functions - Pure function repository for WhatsApp bot with 54+ functions",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --fix",
    "format": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\"",
    "clean": "rm -rf temp/* node_modules/.cache && echo 'Cleaned temp and cache'",
    "clean:all": "rm -rf temp/* node_modules package-lock.json && npm install",
    "stats": "node -e \"const app = require('./index.js'); console.log('Server module loaded')\"",
    "functions:list": "node -e \"require('./index.js').getAvailableFunctions().then(f => { const total = Object.values(f).reduce((s, arr) => s + arr.length, 0); console.log('Total functions:', total); })\"",
    "deploy:render": "render deploy",
    "docker:build": "docker build -t nethunter-fx .",
    "docker:run": "docker run -p 3000:3000 --env-file .env nethunter-fx",
    "docker:clean": "docker system prune -f",
    "backup": "tar -czf backup-$(date +%Y%m%d-%H%M%S).tar.gz fx/ lib/ index.js package.json render.yaml README.md .env.example",
    "update": "npm update --save",
    "audit": "npm audit --audit-level=high",
    "postinstall": "echo 'Nethunter FX installed successfully! Run: npm start'"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "functions",
